<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="usercomms.joemarini.example.com.usercommunications"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="usercomms.joemarini.example.com.usercommunications.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="usercomms.joemarini.example.com.usercommunications.ToastActivity"
            android:label="@string/ToastActivity" />
        <activity
            android:name="usercomms.joemarini.example.com.usercommunications.SnackbarActivity"
            android:label="@string/SnackbarActivity" />
        <activity
            android:name="usercomms.joemarini.example.com.usercommunications.DialogActivity"
            android:label="@string/DialogActivity" />
        <activity
            android:name="usercomms.joemarini.example.com.usercommunications.NotificationActivity"
            android:label="@string/NotificationsActivity" />
        <activity android:name="usercomms.joemarini.example.com.usercommunications.NotificationResultActivity" />
    </application>

</manifest>